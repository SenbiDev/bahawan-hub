{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-use-before-define": "off",
        "no-restricted-globals": "off",
        "no-shadow": "off",
        "no-underscore-dangle": "off"
    }
}
